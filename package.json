{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "local": "set NODE_OPTIONS=--openssl-legacy-provider",
  "remote": "export NODE_OPTIONS=--openssl-legacy-provider",
  "cleanlocal": "rimraf build/static",
  "cleanremote": "rm -rf build",
  "dependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "axios": "0.21.1",
    "fs-extra": "^11.3.0",
    "gadgets360": "file:..",
    "react": "17.0.2",
    "react-bootstrap": "1.6.1",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-redux": "7.2.4",
    "react-router-bootstrap": "0.25.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9",
    "redux-thunk": "2.3.0",
    "rimraf": "^6.0.1",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "clean": "rm -rf build",
    "clear": "node src/cleanBuildStatic.js",
    "move": "node src/moveBuildtoPublic.js",
     "rebuild": "export NODE_OPTIONS=--openssl-legacy-provider && npm run clear && npm run build && npm run move",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider &&   react-scripts start",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && npm install &&  react-scripts build  ",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
